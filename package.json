{
  "name": "reactif",
  "version": "0.0.0-development",
  "description": "Give you the reactivity power just by simply allowing you to interpolate through the the objects by proxying",
  "main": "dist/reactif.common.js",
  "module": "dist/reactif.esm.js",
  "browser": "dist/reactif.umd.js",
  "scripts": {
    "commit": "git-cz",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FrencoJobs/Reactif.git"
  },
  "keywords": [
    "Reactivity",
    "Proxy",
    "Getter",
    "Setter"
  ],
  "author": "Frenco Jobs <frenco.willian@gmail.com> (https://frencojobs.ga)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FrencoJobs/Reactif/issues"
  },
  "homepage": "https://github.com/FrencoJobs/Reactif#readme",
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.4",
    "jest": "23.6.0",
    "regenerator-runtime": "0.12.1",
    "rimraf": "2.6.2",
    "rollup": "0.66.6",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-uglify": "6.0.0",
    "semantic-release": "^15.10.5",
    "travis-deploy-once": "^5.0.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:once"
    }
  }
}
